{
  "name": "mandalay-morning-star",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --bail 1 --no-file-parallelism",
    "test:menu": "vitest run src/components/menu/__tests__/menu-content.test.tsx",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "rls:test": "node scripts/rls-isolation-test.mjs",
    "seed:menu": "npx tsx scripts/seed-menu.ts",
    "verify:menu": "npx tsx scripts/verify-menu.ts",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.startServerCommand='pnpm start' --collect.url=http://localhost:3000",
    "test:animations": "playwright test e2e/animations/",
    "test:a11y": "playwright test e2e/accessibility.spec.ts e2e/animations/v7-accessibility.spec.ts",
    "perf:check": "pnpm build && pnpm lighthouse"
  },
  "dependencies": {
    "@conform-to/react": "^1.15.1",
    "@conform-to/zod": "^1.15.1",
